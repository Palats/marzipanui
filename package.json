{
  "name": "marzipan-ui",
  "version": "0.0.0",
  "description": "Webcomponent marzipan-ui following open-wc recommendations",
  "author": "marzipan-ui",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,es-dev-server \"npm run tsc:watch\" \"es-dev-server --app-index index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start:build": "npm run build && es-dev-server --root-dir dist --app-index index.html --open --compatibility none"
  },
  "dependencies": {
    "@material/mwc-drawer": "^0.17.2",
    "@material/mwc-formfield": "^0.17.2",
    "@material/mwc-icon": "^0.17.2",
    "@material/mwc-icon-button": "^0.17.2",
    "@material/mwc-linear-progress": "^0.17.2",
    "@material/mwc-list": "^0.17.2",
    "@material/mwc-select": "^0.17.2",
    "@material/mwc-snackbar": "^0.17.2",
    "@material/mwc-switch": "^0.17.2",
    "@material/mwc-textfield": "^0.17.2",
    "@material/mwc-top-app-bar": "^0.17.2",
    "@material/mwc-top-app-bar-fixed": "^0.17.2",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "es-dev-server": "^1.5.0",
    "typescript": "~3.8.2",
    "concurrently": "^5.1.0",
    "tslib": "^1.11.0",
    "@open-wc/building-rollup": "^1.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^2.3.4"
  }
}
